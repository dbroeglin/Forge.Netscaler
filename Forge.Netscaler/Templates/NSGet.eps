function <%= $FunctionName %> {
    <#
    .SYNOPSIS
        Gets the specified <%= $Label %> object(s).

    .DESCRIPTION
        Gets the specified <%= $Label %> object(s).
        Either returns a single object identified by its name (-Name parameter)
        or a collection of objects filtered by the other parameters. Those
        filter parameters accept either a literal value or a regexp in the form
        "/someregexp/".

    .EXAMPLE
        <%= $FunctionName %>

        Get all <%= $Label %> objects.

    .EXAMPLE
        <%= $FunctionName %> -Name 'foobar'
    
        Get the <%= $Label %> named 'foobar'.

    .PARAMETER Session
        The NetScaler session object.

    .PARAMETER Name
        The name or names of the <%= $Label %>s to get.

<% $FilterList.GetEnumerator() | % { $Key, $Value = $_.Key, $_.Value %>
    .PARAMETER <%= $Key %>
<% if ($Value[2] -eq "default") { %>
        If true, show builtins. 
           
        Default value: $False
<% } else { %>
        A filter to apply to the <%= $Value[1] %> value.
<% } %>

<% } %>
    #>
    [CmdletBinding(DefaultParameterSetName='get')]
    Param(
        $Session = $Script:Session,

        [Parameter(Position=0, ParameterSetName='get')]
        [string[]]$Name = @()<%= if ($FilterList -and $FilterList.Count -ne 0) { "," } %>

<% $FilterList.GetEnumerator() | ForEach-Object { $i = 0 } {
    $i++
    $Key, $Value = $_.Key, $_.Value
    $Comma = if ($i -eq $FilterList.Count) { "" } else { "," }
%>

        [Parameter(ParameterSetName='search')]
<% if ($Value[2] -eq "default") { %>
        [switch]$<%= $Key + $Comma %>
<% } else { %>
        [string]$<%= $Key + $Comma %>
<% } %>
<% } %>
    )
    Begin {
        _AssertSessionActive
    }

    Process {
<% if ($FilterList -and ($FilterList.Count -ne 0)) { %>
        # Contruct a filter hash if we specified any filters
        $Filters = @{}
<% $FilterList.GetEnumerator() | % { $Key, $Value = $_.Key, $_.Value %>
<% if ($Value[2] -eq "default") { %>
        if (!$<%= $Key %>) {
            $Filters['<%= $Value[0] %>'] = '<%= $Value[3] %>'
        }
<% } else { %>
        if ($PSBoundParameters.ContainsKey('<%= $Key %>')) {
            $Filters['<%= $Value[0] %>'] = $<%= $Key %>
        }
<% } %>
<% } %>
        _InvokeNSRestApiGet -Session $Session -Type <%= $Type %> -Name $Name <%= if ($Arguments) { "-Arguments" } else { "-Filters" } %> $Filters
<% } else { %>
        _InvokeNSRestApiGet -Session $Session -Type <%= $Type %> -Name $Name
<% } %>
    }
}